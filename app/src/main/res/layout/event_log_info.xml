        android:text="Manual ID"
        android:textColor="@color/Black"
        android:textSize="14sp"
        android:textStyle="bold" />

    <TableLayout
        android:id="@+id/LogInfoTab_tableRow1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:stretchColumns="*" >

        <TableRow
            android:id="@+id/LogInfoTab_tableRow"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="@color/DarkGray2" >

            <TextView
                android:id="@+id/LogInfoTab_n1st_row_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:paddingTop="3dp"
                android:text="@string/event"
                android:textColor="@color/BrightBlue"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/LogInfoTab_n1st_row_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:paddingTop="3dp"
                android:text="None"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/LogInfoTab_n1st_row_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:paddingTop="3dp"
                android:text="@string/date"
                android:textColor="@color/BrightBlue"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/LogInfoTab_n1st_row_4"
