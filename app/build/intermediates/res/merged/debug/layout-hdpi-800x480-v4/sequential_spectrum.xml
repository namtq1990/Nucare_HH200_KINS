<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_weight="1"
    android:background="#161615"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="bottom"
        android:layout_weight="0.01"
        android:background="#161615"
        android:orientation="horizontal" >

        <RelativeLayout
            android:id="@+id/RelativieLayout1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="top|bottom"
            android:background="#161615"
            android:orientation="vertical" >

            <ViewFlipper
                android:id="@+id/flipper"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_alignParentTop="true"
                android:layout_marginBottom="7dp"
                android:layout_weight="0.07" >

                <FrameLayout
                    android:id="@+id/frameLayout1"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_gravity="top"
                    android:layout_marginTop="0dp" >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="24dp"
                        android:layout_gravity="top"
                        android:layout_marginTop="0.5dp"
                        android:background="@color/RealBlack"
                        android:orientation="horizontal" >

                        <TextView
                            android:id="@+id/sequential_mode"
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"
                            android:layout_gravity="left"
                            android:layout_marginLeft="7.5dp"
                            android:fontFamily="sans-serif-light"
                            android:gravity="center_vertical"
                            android:paddingBottom="2dp"
                            android:text="Measurement In Progress..."
                            android:textColor="@color/Brown2"
                            android:textSize="12sp" />
                    </LinearLayout>

                    <TextView
                        android:id="@+id/sequential_count"
                        android:layout_width="40dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="594dp"
                        android:layout_marginRight="7.5dp"
                        android:layout_marginTop="3.5dp"
                        android:fontFamily="sans-serif-light"
                        android:text="0 / 0"
                        android:textColor="@color/Brown2"
                        android:textSize="12sp" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:layout_gravity="top"
                        android:layout_marginTop="30dp"
                        android:background="#171616"
                        android:orientation="horizontal" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="100dp"
                            android:fontFamily="sans-serif-light"
                            android:text="Dose. rate"
                            android:textColor="@color/Brown"
                            android:textSize="12sp" />

                        <TextView
                            android:id="@+id/avg_Doserate_txt"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="7.5dp"
                            android:fontFamily="sans-serif-light"
                            android:text="0"
                            android:textColor="@color/White"
                            android:textSize="20sp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="sans-serif-light"
                            android:text=" usv/h"
                            android:textColor="@color/White"
                            android:textSize="12sp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="7.5dp"
                            android:fontFamily="sans-serif-light"
                            android:text="CPS"
                            android:textColor="@color/Brown"
                            android:textSize="12sp" />

                        <TextView
                            android:id="@+id/cpsTxt"
                            android:layout_width="37.5dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="7.5dp"
                            android:fontFamily="sans-serif-light"
                            android:gravity="left"
                            android:text="0.0 k"
                            android:textColor="@color/White"
                            android:textSize="12sp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="sans-serif-light"
                            android:text="Total Count"
                            android:textColor="@color/Brown"
                            android:textSize="12sp" />

                        <TextView
                            android:id="@+id/totalCountTxt"
                            android:layout_width="43.5dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="7.5dp"
                            android:fontFamily="sans-serif-light"
                            android:text="0.0 k"
                            android:textColor="@color/White"
                            android:textSize="12sp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="sans-serif-light"
                            android:text="Acq. Time"
                            android:textColor="@color/Brown"
                            android:textSize="12sp" />

                        <TextView
                            android:id="@+id/Acq_TimeTxt"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="7.5dp"
                            android:fontFamily="sans-serif-light"
                            android:gravity="left"
                            android:text="0"
                            android:textColor="@color/White"
                            android:textSize="18sp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="3dp"
                            android:fontFamily="sans-serif-light"
                            android:gravity="left"
                            android:text="/"
                            android:textColor="@color/White"
                            android:textSize="18sp" />

                        <TextView
                            android:id="@+id/Acq_TimeTxt2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="3dp"
                            android:fontFamily="sans-serif-light"
                            android:gravity="left"
                            android:text="0"
                            android:textColor="@color/White"
                            android:textSize="18sp"
                            android:textStyle="normal" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="sans-serif-light"
                            android:text=" s"
                            android:textColor="@color/White"
                            android:textSize="10sp" />
                    </LinearLayout>

                    <view
                        android:id="@+id/IDspectrum"
                        android:layout_width="fill_parent"
                        android:layout_height="252dp"
                        android:layout_gravity="bottom"
                        android:layout_marginBottom="0dp"
                        class="android.HH100.Control.SpectrumView" />
                </FrameLayout>

                <LinearLayout
                    android:id="@+id/content2"
                    android:layout_width="fill_parent"
                    android:layout_height="match_parent" >

                    <FrameLayout
                        android:id="@+id/content"
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:background="@color/RealBlack" >

                        <view
                            android:id="@+id/Iso_analysis"
                            android:layout_width="fill_parent"
                            android:layout_height="70dp"
                            android:layout_gravity="top"
                            class="android.HH100.Control.Analysis_TopInfor" />

                        <ScrollView
                            android:id="@+id/IsoScrView"
                            android:layout_width="match_parent"
                            android:layout_height="fill_parent"
                            android:paddingTop="70dp" >

                            <LinearLayout
                                android:id="@+id/AdLayout"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical" >
                            </LinearLayout>
                        </ScrollView>
                    </FrameLayout>
                </LinearLayout>
            </ViewFlipper>

            <RelativeLayout
                android:id="@+id/filperTouch"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_alignParentLeft="true"
                android:layout_marginBottom="2dp"
                android:layout_marginRight="6dp"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/filperImgView"
                    android:layout_width="48dp"
                    android:layout_height="15dp"
                    android:background="@drawable/right"
                    android:scaleType="fitXY"
                    android:scaleX="0.5"
                    android:scaleY="0.9" />
            </RelativeLayout>
        </RelativeLayout>

        <LinearLayout
            android:id="@+id/frameLayout2"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_gravity="right"
            android:orientation="vertical"
            android:visibility="gone" >

            <TextView
                android:id="@+id/textView1"
                android:layout_width="wrap_content"
                android:layout_height="80dp"
                android:layout_weight="8"
                android:text=""
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textSize="8sp" />

            <Button
                android:id="@+id/SecUpBtn"
                style="?android:attr/buttonStyleSmall"
                android:layout_width="fill_parent"
                android:layout_height="50dp"
                android:layout_marginLeft="3dp"
                android:layout_marginRight="3dp"
                android:layout_weight="1"
                android:background="@drawable/timeup"
                android:soundEffectsEnabled="true"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/ManualID_time"
                android:layout_width="fill_parent"
                android:layout_height="60dp"
                android:layout_marginLeft="3dp"
                android:layout_marginRight="3dp"
                android:layout_weight="1"
                android:background="@drawable/picker_center"
                android:gravity="center"
                android:text="30"
                android:textAlignment="center"
                android:textColor="@color/Black"
                android:textSize="17sp" />

            <Button
                android:id="@+id/SecDownBtn"
                style="?android:attr/buttonStyleSmall"
                android:layout_width="fill_parent"
                android:layout_height="50dp"
                android:layout_marginLeft="3dp"
                android:layout_marginRight="3dp"
                android:layout_weight="1"
                android:background="@drawable/timedown"
                android:soundEffectsEnabled="true"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="80dp"
                android:layout_weight="6"
                android:text="" />

            <Button
                android:id="@+id/ManualID_Cancel"
                style="?android:attr/buttonStyleSmall"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:soundEffectsEnabled="true"
                android:text="Cancel"
                android:textSize="10sp"
                android:visibility="invisible" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/layout_sequenceInfo"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_gravity="right"
            android:orientation="vertical"
            android:visibility="gone" >

            <View
                android:layout_width="wrap_content"
                android:layout_height="30dp" />

            <TextView
                android:id="@+id/tv_repeat_count"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:gravity="center"
                android:text="Count"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textSize="11sp" />

            <TextView
                android:id="@+id/tv_seq_repeat"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="3"
                android:gravity="center"
                android:text="3/5"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textColor="@color/orange"
                android:textSize="16sp"
                android:textStyle="bold" />

            <View
                android:layout_width="wrap_content"
                android:layout_height="30dp" />

            <TextView
                android:id="@+id/tv_goal_time2"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:gravity="center"
                android:text="Measurement Time"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textSize="11sp" />

            <TextView
                android:id="@+id/tv_goal_time"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:text="15/30 sec"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textSize="13sp"
                android:textStyle="bold" />

            <View
                android:layout_width="wrap_content"
                android:layout_height="140dp" />

            <Button
                android:id="@+id/btn_seq_Cancel"
                style="?android:attr/buttonStyleSmall"
                android:layout_width="fill_parent"
                android:layout_height="0dp"
                android:text="Cancel"
                android:textSize="10sp" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="0.99"
        android:background="#161615"
        android:orientation="horizontal" >

        <view
            android:id="@+id/SetupSpcSrc_ProgressBar"
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:layout_gravity="bottom"
            class="android.HH100.Control.ProgressBar"
            android:background="@color/WhiteGray2" />
    </LinearLayout>

</LinearLayout>